{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "jest"
  ],
  "rules": {
    "react/jsx-filename-extension": [0],
    "import/prefer-default-export": [0],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  },
  "globals": {
    "fetch": false,
    "Headers": false,
    "Response": false,
  },
  "env": {
    "jest/globals": true
  }
}
