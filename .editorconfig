# EditorConfig http://editorconfig.org

root = true

[{*.js,*.mjs,*.jsx,*.json,*.sh,*.css,*.html}]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# do not format these files
[{**/cSpell.json,**/node_modules/**,dist/**,.tmp/**,**/test/lodash/**,**/test/fixtures/**}]
indent_style = none
indent_size = none
end_of_line = none
charset = none
trim_trailing_whitespace = false
insert_final_newline = false
